{
    "conditions": {
    	"executortype": "container-executor",
        "nodes": 1,
        "processespernode": 1,
	    "mem": "10Gi",
        "cpu": "1000m",
        "gpu": {
            "count": 0
        },
	"walltime": 1100
    },
    "maxexectime": 1000,
    "maxretries": 3,
    "funcname": "execute",
    "kwargs": {
        "cmd": "cd ",
        "docker-image": "henrikforsgren/openeo:new",
		"args": [
            "cfs/my_cloud_filtering/ ; python3 main.py"
		]
    },
	"env": {
		"coords": "{\"east\": 14.79187736312752, \"south\": 55.991257253340635, \"west\": 14.555719745816692, \"north\": 56.10331290101734}",
        "date": "2022-02-01",
		"data_source": "l2a"
	},
    "fs": {
        "mount": "/cfs",
        "dirs": [
            {
                "label": "/my_cloud_filtering",
                "dir": "/my_cloud_filtering",
                "keepfiles": false,
                "onconflicts": {
                    "onstart": {
                        "keeplocal": false
                    },
                    "onclose": {
                        "keeplocal": false 
                    }
                }
            },
            {
                "label": "/models",
                "dir": "/models",
                "keepfiles": false,
                "onconflicts": {
                    "onstart": {
                        "keeplocal": false
                    },
                    "onclose": {
                        "keeplocal": false 
                    }
                }
            },
            {
                "label": "/outputs",
                "dir": "/outputs",
                "keepfiles": false,
                "onconflicts": {
                    "onstart": {
                        "keeplocal": false
                    },
                    "onclose": {
                        "keeplocal": true 
                    }
                }
            }
        ]
    }
}
